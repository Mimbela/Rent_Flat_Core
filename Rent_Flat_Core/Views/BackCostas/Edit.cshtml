@model Rent_Flat_Core.Models.Costas

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/PlantillaBack.cshtml";
}

<h2>Edit</h2>

<h4>Costas</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" hidden>
                <label asp-for="Cod_Provincia" class="control-label"></label>
                <input asp-for="Cod_Provincia" class="form-control" />
                <span asp-validation-for="Cod_Provincia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NombreProvincia" class="control-label"></label>
                <input asp-for="NombreProvincia" class="form-control" />
                <span asp-validation-for="NombreProvincia" class="text-danger"></span>
            </div>
            <div class="form-group" hidden>
                <label asp-for="Foto" class="control-label"></label>
                <input asp-for="Foto" class="form-control" />
                <span asp-validation-for="Foto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Nombre_Costas">Back to List</a>
</div>

